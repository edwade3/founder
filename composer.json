{
    "name": "dyrynda/founder",
    "description": "Founder is a starter project for your Laravel applications.",
    "keywords": ["laravel", "starter"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "dyrynda/laravel-make-user": "^2.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "^0.8.0"
    },
    "suggest": {
        "dyrynda/laravel-model-uuid": "For applications that need globally-unique model identifiers"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "sensiolabs/security-checker": "^4.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "check-security": [
            "vendor/bin/security-checker security:check"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
